{
  "name": "quasr-projects",
  "version": "0.0.1",
  "description": "A quasar tasks app",
  "productName": "Quasar Projects",
  "author": "robert",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue ./",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build",
    "deploy": "npm run build && aws s3 sync dist/spa s3://kanban-web --acl public-read",
    "android": "npx cap run android",
    "sync": "npx cap sync",
    "build-android": "ionic capacitor build android",
    "update-icon-and-splash": "npx capacitor-assets generate",
    "show-logs": "logkitty android 'console.log'",
    "ionic:build": "npm run build",
    "view-keystore": "keytool -list -v -alias androiddebugkey -keystore ~/.android/debug.keystore"
  },
  "dependencies": {
    "@capacitor/android": "^6.1.0",
    "@capacitor/core": "^6.1.0",
    "@codetrix-studio/capacitor-google-auth": "^3.4.0-rc.4",
    "@quasar/extras": "^1.16.4",
    "@tinymce/tinymce-vue": "^5.1.1",
    "axios": "^1.2.1",
    "pinia": "^2.0.11",
    "pinia-persist": "^0.1.0",
    "quasar": "^2.6.0",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.1.0",
    "@quasar/app-vite": "^1.3.0",
    "autoprefixer": "^10.4.2",
    "dotenv": "^16.4.5",
    "electron": "^28.1.3",
    "electron-packager": "^17.1.2",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "postcss": "^8.4.14",
    "prettier": "^2.5.1"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
